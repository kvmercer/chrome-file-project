{
  "manifest_version": 2,
  "name": "Video Downloader",
  "description": "Download videos on a site",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/static/js/background.js"]
    }
  ],
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "permissions": [],
  "content_security_policy": "script-src 'self' 'sha256-zOKyoHKGoLiOxYU3o+hx5CdWWqakXIKS4RnrnMpQ9+E='"
}

